  },
  "dependencies": {
    "async": "~0.2.9",
    "colors": "~0.6.2",
    "debug": "~0.7.4",
    "glob": "~3.2.7",
    "js-yaml": "~3.0.1",
    "optimist": "~0.6.0",
    "underscore": "~1.5.2",
    "wrench": "~1.5.4"
  },
  "devDependencies": {
    "grunt": "~0.4.2",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-jshint": "~0.8.0",
    "grunt-contrib-nodeunit": "~0.2.0",
    "grunt-contrib-yuidoc": "~0.5.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-exec": "~0.4.3",
    "coveralls": "~2.6.1",
    "istanbul": "~0.2.3"
  },
  "keywords": [
    "svg",
    "png",
    "inkscape"
  ]
